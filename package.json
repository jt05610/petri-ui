{
  "name": "petri",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "node_modules/.bin/remix build",
    "dev": "node_modules/.bin/remix watch",
    "prod": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production node_modules/.bin/remix watch",
    "dev:serve": "node_modules/.bin/binode --require ./mocks -- @remix-run/serve:remix-serve ./build",
    "format": "node_modules/.bin/prettier --write .",
    "format:repo": "npm run format && npm run lint:repo -- --fix",
    "lint": "node_modules/.bin/eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "npm run lint -- --config .eslintrc.repo.js",
    "setup": "node_modules/.bin/prisma generate && prisma migrate deploy && prisma db seed",
    "start": "pnpm build && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production ts-node --require tsconfig-paths/register server/index.ts",
    "start:prod": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production concurrently \"ts-node --require tsconfig-paths/register server/index.ts\" \"pnpm run dev\"",
    "start:dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development concurrently \"ts-node --require tsconfig-paths/register server/index.ts\" \"pnpm run dev\"",
    "start:mocks": "node_modules/.bin/binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "node_modules/.bin/vitest",
    "test:e2e:dev": "node_modules/.bin/start-server-and-test dev http://localhost:3000 \\\"npx cypress open\\\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "node_modules/.bin/cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \\\"npx cypress run\\\"",
    "typecheck": "node_modules/.bin/tsc && tsc -p cypress",
    "validate": "node_modules/.bin/run-p \\\"test -- --run\\\" lint typecheck test:e2e:run",
    "makeQueries": "ts-node --require tsconfig-paths/register prisma/queries.ts && sqlc generate",
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@conform-to/react": "^0.7.4",
    "@conform-to/zod": "^0.7.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@npmcli/fs": "^3.1.0",
    "@paralleldrive/cuid2": "^2.2.1",
    "@prisma/client": "^5.1.1",
    "@remix-run/css-bundle": "^1.19.1",
    "@remix-run/express": "^1.19.1",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/router": "^1.7.2",
    "@remix-run/serve": "^1.19.1",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^8.2.0",
    "cuid": "^3.0.0",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^6",
    "graphviz-react": "^1.2.5",
    "isbot": "^3.6.12",
    "jest-mock-extended": "^3.0.5",
    "kill-port": "^2.0.1",
    "morgan": "^1.10.0",
    "prisma-zod-generator": "^0.8.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "svg-pan-zoom": "^3.6.1",
    "tiny-invariant": "^1.3.1",
    "use-context-selector": "^1.4.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/amqplib": "^0.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.40.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.16.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "happy-dom": "^9.20.3",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^5.1.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-json-schema": "^0.59.0",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
