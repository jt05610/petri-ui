{
  "name": "petri",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "node_modules/.bin/remix build",
    "dev": "node_modules/.bin/remix dev",
    "dev:serve": "node_modules/.bin/binode --require ./mocks -- @remix-run/serve:remix-serve ./build",
    "format": "node_modules/.bin/prettier --write .",
    "format:repo": "npm run format && npm run lint:repo -- --fix",
    "lint": "node_modules/.bin/eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "npm run lint -- --config .eslintrc.repo.js",
    "setup": "node_modules/.bin/prisma generate && prisma migrate deploy && prisma db seed",
    "start": "node_modules/.bin/remix-serve build",
    "start:mocks": "node_modules/.bin/binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "node_modules/.bin/vitest",
    "test:e2e:dev": "node_modules/.bin/start-server-and-test dev http://localhost:3000 \\\"npx cypress open\\\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "node_modules/.bin/cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \\\"npx cypress run\\\"",
    "typecheck": "node_modules/.bin/tsc && tsc -p cypress",
    "validate": "node_modules/.bin/run-p \\\"test -- --run\\\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@jsonforms/core": "^3.1.0",
    "@jsonforms/material-renderers": "^3.1.0",
    "@jsonforms/react": "^3.1.0",
    "@mui/material": "^5.14.2",
    "@prisma/client": "^5.0.0",
    "@remix-run/css-bundle": "^1.19.1",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/router": "^1.7.2",
    "@remix-run/serve": "^1.19.1",
    "@ts-graphviz/react": "^0.9.1",
    "@types/react-test-renderer": "^18.0.0",
    "bcryptjs": "^2.4.3",
    "isbot": "^3.6.12",
    "mermaid": "^10.3.0",
    "npmcli": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "svg-pan-zoom": "^3.6.1",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.40.2",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.16.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "happy-dom": "^9.20.3",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^5.0.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-json-schema": "^0.59.0",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
